{
  "name": "frpc",
  "version": "1.0.10",
  "slug": "frpc",
  "description": "Browser the Home Assistant From Internet",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/jdbwlj/hassio-frpc",
  "startup": "services",
  "boot": "auto",
  "image": "zhujisheng/{arch}-addon-frpc",
  "options": {
    "frp_server": "frp_服务器",
    "frp_server_port": 7000,
    "frp_token": "frp_服务器_token",
    "local_host": "homeassistant",
    "local_port": 8123,
    "tunnel_type": "http",
    "http_domain": "xxx.xxx.xxx"
  },
  "schema": {
    "frp_server": "str",
    "frp_server_port": "port",
    "frp_token": "str",
    "local_host": "str",
    "local_port": "port",
    "tunnel_type": "list(tcp|http)",
    "http_domain": "str?",
    "http_subdomain_host": "str?",
    "tcp_remote_port": "port?"
  }
}
